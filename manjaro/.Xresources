Xft.antialias:              true
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle:              hintslight

! black
*.color0:       #272827
*.color8:       #272827

! red
*.color1:       #6673bf
*.color9:       #899aff

! green
*.color2:       #3ea290
*.color10:      #52ad91

! yellow
*.color3:       #b0ead9
*.color11:      #98c9bb

! blue
*.color4:       #31658c
*.color12:      #477ab3

! magenta
*.color5:       #596196
*.color13:      #7882bf

! cyan
*.color6:       #8292b2
*.color14:      #95a7cc

! white
*.color7:       #c8cacc
*.color15:      #edeff2

URxvt.font:                       xft:Hack:style=Regular:pixelsize=12:antialias=true, xft:Source \Code \Pro \Medium:pixelsize=12:antialias=true, xft:Emojione, xft:DejaVu Sans:pixelsize=12

URxvt*letterSpace: 0

URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*background:                 #040B12
URxvt*foreground:                 #ffffff
URxvt*transparent:		  false
URxvt*saveLines:                  10000

Rxvt.perl-ext-common:            ...,clipboard,font-size

