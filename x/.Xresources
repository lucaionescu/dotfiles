Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

XTerm*background: #bb0000
XTerm*foreground: #e7e7e7
XTerm*pointerColor: #8ABEB7
XTerm*faceName: Source \Code \Pro \Medium
XTerm*faceSize: 9

*fading:                          15
*fadeColor:                       black
*pointerColorBackground:          #2B2C2B
*pointerColorForeground:          #8ABEB7

! special
*.foreground:   #ffffff
*.background:   #1b1b1b
*.cursorColor:  #ffffff

! black
*.color0:       #272827
*.color8:       #272827

! red
*.color1:       #6673bf
*.color9:       #899aff

! green
*.color2:       #3ea290
*.color10:      #52ad91

! yellow
*.color3:       #b0ead9
*.color11:      #98c9bb

! blue
*.color4:       #31658c
*.color12:      #477ab3

! magenta
*.color5:       #596196
*.color13:      #7882bf

! cyan
*.color6:       #8292b2
*.color14:      #95a7cc

! white
*.color7:       #c8cacc
*.color15:      #edeff2


Xcursor.theme: Adwaita
Xcursor.size:                     0

XTerm*background:                 #272827
XTerm*foreground:                 #fdf6e3
XTerm*reverseVideo:               on
XTerm*faceName:                   xft:Source \Code \Pro \Medium:pixelsize=14:antialias=true
XTerm*selectToClipboard:          true

!URxvt.font:                       xft:Source \Code \Pro \Medium:pixelsize=14:antialias=true
URxvt.font:                       xft:Hack:style=Regular:pixelsize=12:antialias=true, xft:Source \Code \Pro \Medium:pixelsize=12:antialias=true, xft:Emojione, xft:DejaVu Sans:pixelsize=12

