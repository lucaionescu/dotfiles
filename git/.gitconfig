[user]
	email = luca.ionescu@gmx.de
	name = Luca Ionescu

[github]
	user = lucaionescu

[alias]
	br = branch
	ci = commit 
	l = log --graph --abbrev-commit --decorate --date=short --format=format:'%C(red)%h%C(reset) - %C(blue)(%ai) %C(reset) %C(yellow)%s%C(reset) %C(yellow)- %an%C(reset)%C(red)%d%C(reset)' --all
	s = status
	
[credential]
	helper = cache

[core]
	editor = "emacs -nw"
	excludesfile = ~/.gitignore_global

[help]
	autocorrect = 1

[color]
	ui = auto
	
